@model IEnumerable<MyClass.Models.Category>
<div class=" rsidebar span_1_of_left">
    <h3 class="cate">Loại sản phẩm</h3>
    <div class="menu-drop">
        @foreach (var category in Model)
        {
            <ul class="cute" >
                <li class="cute ">
                    <a href="~/@category.Slug">@category.Name </a>
                </li>
            </ul>
        }
    </div>
</div>
